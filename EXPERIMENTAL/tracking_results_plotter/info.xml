<?xml version="1.0" encoding="UTF-8"?>
<plugin name="Tracking Results Plotter">
    <directory>
        tracking_results_plotter
    </directory>
    <version>
        1.0.0
    </version>
    <author>
        Assistant
    </author>
    <url>
        https://github.com/flika-org/tracking_results_plotter/archive/master.zip
    </url>
    <dependencies>
        <dependency name='numpy'></dependency>
        <dependency name='pandas'></dependency>
        <dependency name='matplotlib'></dependency>
        <dependency name='scipy'></dependency>
        <dependency name='qtpy'></dependency>
    </dependencies>
    <menu_layout>
        <action location="__init__" function="launch_tracking_plotter">Launch Results Plotter</action>
    </menu_layout>
</plugin>
